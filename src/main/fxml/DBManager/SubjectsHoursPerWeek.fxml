<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane animated="false" collapsible="false" text="Введите допустимую нагрузку для предметов в неделю (в часах)" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controllers.DBManager.SubjectsHoursPerWeekController">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Label text="Минимальная" GridPane.rowIndex="1" />
        <Label text="Максимальная" GridPane.rowIndex="2" />
        <TextField fx:id="min" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <TextField fx:id="max" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <Button mnemonicParsing="false" text="Ок" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        <ChoiceBox fx:id="subjects" prefWidth="150.0" GridPane.columnIndex="1" />
        <Label text="Выберите предмет" />
    </GridPane>
</TitledPane>
